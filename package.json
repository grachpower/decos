{
  "name": "dects",
  "version": "1.0.0",
  "description": "typescript additional decorators",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run lint && jest",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "build": "npm run clean:dist && npm run test && tsc ./src/**.ts --outDir lib",
    "rimraf": "rimraf",
    "clean:dist": "npm run rimraf -- lib",
    "start:sandbox": "ts-node ./sandbox/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grachpower/decos.git"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "typescript",
    "js"
  ],
  "author": "Duke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grachpower/decos/issues"
  },
  "homepage": "https://github.com/grachpower/decos#readme",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.6",
    "typescript": "^2.8.3",
    "webpack": "^4.8.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@types/jasmine": "^2.8.7",
    "@types/node": "^10.0.8",
    "@types/webpack": "^4.1.6",
    "codelyzer": "^4.3.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "save-dev": "^2.0.0",
    "ts-node": "^6.0.3",
    "tslib": "^1.9.0",
    "tslint": "^5.10.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
